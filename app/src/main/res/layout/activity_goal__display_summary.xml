<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Goals.Goal_DisplaySummary"
    android:fitsSystemWindows="true"
    android:id="@+id/layout_drawer"
    android:background="#b6e1df">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <include
            android:id="@+id/toolbar"
            layout="@layout/nav_toolbar" />

        <TextView
            android:id="@+id/txt_1"
            android:layout_width="411dp"
            android:layout_height="66dp"
            android:fontFamily="@font/montserrat"
            android:text="Your summary report and reflection..."
            android:textColor="@android:color/white"
            android:textSize="25dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbar" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="411dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_1">


            <View
                android:id="@+id/view"
                android:layout_width="392dp"
                android:layout_height="4dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:background="@android:color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <View
                android:id="@+id/view2"
                android:layout_width="4dp"
                android:layout_height="520dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:background="@android:color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/view3"
                android:layout_width="4dp"
                android:layout_height="514dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="10dp"
                android:background="@android:color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <View
                android:id="@+id/bar_left"
                android:layout_width="392dp"
                android:layout_height="4dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="516dp"
                android:layout_marginEnd="10dp"
                android:background="@android:color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txt_2"
                android:layout_width="136dp"
                android:layout_height="22dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:fontFamily="@font/montserrat"
                android:text="Congratulations!"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/txt_3"
                android:layout_width="119dp"
                android:layout_height="22dp"
                android:layout_below="@+id/txt_2"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="@string/Summary_1"
                android:textSize="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view" />

            <TextView
                android:id="@+id/display_goal"
                android:layout_width="wrap_content"
                android:layout_height="22dp"
                android:layout_below="@+id/txt_3"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/montserrat_bold"
                android:hint="@string/Summary_1"
                android:textSize="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_3" />

            <TextView
                android:id="@+id/txt_4"
                android:layout_width="296dp"
                android:layout_height="22dp"
                android:layout_below="@+id/display_goal"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="@string/Summary_2_2"
                android:textSize="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.495"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/display_goal" />

            <TextView
                android:id="@+id/display_reason"
                android:layout_width="wrap_content"
                android:layout_height="22dp"
                android:layout_below="@+id/txt_4"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="148dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="110dp"
                android:fontFamily="@font/montserrat_bold"
                android:hint="display_reason"
                android:textSize="16dp"
                app:layout_constraintEnd_toStartOf="@+id/view3"
                app:layout_constraintStart_toEndOf="@+id/view2"
                app:layout_constraintTop_toBottomOf="@+id/txt_4" />


            <Button
                android:id="@+id/btn_view_roadmap"
                android:layout_width="200dp"
                android:layout_height="48dp"
                android:layout_below="@+id/display_reason"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="112dp"
                android:layout_marginEnd="71dp"
                android:background="@drawable/button_shape"
                android:onClick="btn_view_roadmap"
                android:text="View your RoadMap Steps"
                app:layout_constraintEnd_toStartOf="@+id/view3"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/view2"
                app:layout_constraintTop_toBottomOf="@+id/display_reason" />

            <Button
                android:id="@+id/button"
                android:layout_width="300dp"
                android:layout_height="48dp"
                android:layout_below="@+id/btn_view_roadmap"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="63dp"
                android:layout_marginTop="2dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/button_shape"
                android:onClick="btnviewroadmaphazardicon"
                android:text="View what you have identified as potentially difficult"
                app:layout_constraintEnd_toStartOf="@+id/view3"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/view2"
                app:layout_constraintTop_toBottomOf="@+id/btn_view_roadmap" />

            <TextView
                android:id="@+id/display_date_tv"
                android:layout_width="369dp"
                android:layout_height="45dp"
                android:layout_below="@+id/btn_view_roadmap"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="32dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="You will take your first step in making this goal a reality on:"
                android:textAlignment="center"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/view2"
                app:layout_constraintTop_toBottomOf="@+id/button" />


            <TextView
                android:id="@+id/display_input_date_time"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="185dp"
                android:layout_marginTop="2dp"
                android:layout_marginEnd="162dp"
                android:fontFamily="@font/montserrat"
                android:hint="date"
                android:textSize="16sp"
                app:layout_constraintEnd_toStartOf="@+id/view3"
                app:layout_constraintStart_toEndOf="@+id/view2"
                app:layout_constraintTop_toBottomOf="@+id/display_date_tv" />

            <TextView
                android:id="@+id/display_input_date_day"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="185dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="162dp"
                android:fontFamily="@font/montserrat"
                android:hint="date"
                android:textSize="16sp"
                app:layout_constraintEnd_toStartOf="@+id/view3"
                app:layout_constraintStart_toEndOf="@+id/view2"
                app:layout_constraintTop_toBottomOf="@+id/display_input_date_time" />

            <TextView
                android:id="@+id/display_input_date_month"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_below="@id/txt_3"
                android:layout_marginStart="185dp"
                android:layout_marginTop="3dp"
                android:layout_marginEnd="144dp"

                android:fontFamily="@font/montserrat"
                android:hint="month"
                android:textSize="16sp"
                app:layout_constraintEnd_toStartOf="@+id/view3"
                app:layout_constraintStart_toEndOf="@+id/view2"
                app:layout_constraintTop_toBottomOf="@+id/display_input_date_day" />

            <TextView
                android:id="@+id/txt_5"
                android:layout_width="392dp"
                android:layout_height="143dp"
                android:layout_below="@id/txt_4"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="2dp"
                android:layout_marginTop="1dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="@string/Summary_2"
                android:textAlignment="center"
                android:textSize="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/display_input_date_month" />

            <Button

                android:id="@+id/btn_email"
                android:layout_width="230dp"
                android:layout_height="50dp"
                android:layout_below="@id/bar_left"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="7dp"
                android:background="@drawable/button_shape"
                android:fontFamily="@font/montserrat"
                android:onClick="btn_email"
                android:text="Click here to email a copy of your plan to yourself"
                android:textAllCaps="false"
                android:textColor="#326b6f"
                android:textSize="15dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.591"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bar_left" />


            <Button
                android:id="@+id/btn_home"
                android:layout_width="230dp"
                android:layout_height="50dp"
                android:layout_below="@+id/btn_email"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="69dp"
                android:background="#696969"
                android:fontFamily="@font/montserrat"
                android:onClick="btn_home"
                android:text="HOME"

                android:textColor="#ffff"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btn_email" />

            <EditText
                android:id="@+id/ed"
                android:layout_width="72dp"
                android:layout_height="80dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="34dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Name"
                app:layout_constraintEnd_toStartOf="@+id/btn_email"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view2" />


        </androidx.constraintlayout.widget.ConstraintLayout>


